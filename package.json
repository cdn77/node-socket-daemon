{
  "name": "nodesockd",
  "version": "2.0.0-rc.5",
  "description": "Bridges a Nginx reverse proxy to a Node.js service using Unix sockets",
  "homepage": "https://github.com/cdn77/node-socket-daemon",
  "license": "MIT",
  "author": {
    "name": "Dan Kadera",
    "email": "dan.kadera@cdn77.com",
    "url": "https://www.cdn77.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cdn77/node-socket-daemon.git"
  },
  "dependencies": {
    "@debugr/console": "^3.0.0-rc.8",
    "@debugr/core": "^3.0.0-rc.9",
    "js-yaml": "^4.1.0",
    "semver": "^7.3.8",
    "uuid": "^9.0.0",
    "yargs": "^17.7.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.15.10",
    "@types/semver": "^7.3.13",
    "@types/uuid": "^9.0.1",
    "@types/yargs": "^17.0.24",
